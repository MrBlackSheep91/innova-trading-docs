{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "InnovaTrading API",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#3B82F6",
    "light": "#60A5FA",
    "dark": "#1E40AF"
  },
  "topbarLinks": [
    {
      "name": "Dashboard",

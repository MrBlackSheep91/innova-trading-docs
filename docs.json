    "name": "Get API Key",
    "url": "https://innova-trading.com/settings/api"
  },
  "anchors": [
    {
      "name": "GitHub",
      "icon": "github",
      "url": "https://github.com/InnovaTrading"
    },
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://discord.gg/innovatrading"
    }
  ],
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "concepts/signals",
              "concepts/indicators",
              "concepts/timeframes"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/first-indicator",
              "guides/complete-examples",
              "guides/signal-best-practices",
              "guides/continuous-integration"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Market Data",
            "pages": [
              "api-reference/market-data/get-bars",
              "api-reference/market-data/get-symbols"
            ]
          },
          {
            "group": "External Indicators",
            "pages": [
              "api-reference/indicators/submit-indicator",
              "api-reference/indicators/get-indicator",
              "api-reference/indicators/delete-indicator",
              "api-reference/indicators/list-indicators"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
